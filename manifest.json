{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/zoom-48.png",
    "128": "icons/zoom-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/*"],
      "js": ["redirect.js"]
    }
  ]
}
